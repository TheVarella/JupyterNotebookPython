    "# Pedir ao usuário para digitar a idade\n",
    "idade = int(input(\"Digite sua idade: \"))\n",
    "\n",
    "# Verificar a categoria de idade\n",
    "if idade < 18:\n",
    "    print(\"Você é menor de idade.\")\n",
    "elif idade >= 18 and idade < 60:\n",
    "    print(\"Você é adulto.\")\n",
    "else:\n",
    "    print(\"Você é idoso.\")\n"
